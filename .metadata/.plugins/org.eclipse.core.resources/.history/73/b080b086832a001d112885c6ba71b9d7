
public class Ifgrammar2 {
	public static void main(String[] args) {
		boolean isMarried = true;
		boolean isMan = false;
		
		String str;
		
		str = isMarried ? "결혼했다" : "결혼 안 했다";
		
		System.out.println(str);
		
		switch (str) {
		case "결혼했다"
		System.out.println("O");
		break;
		case "결혼 안했다":
			System.out.println("X");
			break;
			default;
			System.out.println("X");
		}
		
	}

}
